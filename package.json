{
  "name": "curious-cat",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "edge:generate": "turbo edge:generate:js edge:generate:interfaces",
    "edge:new:migration": "turbo edge:new:migration",
    "edge:migrate": "turbo edge:migrate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.2.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.34",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "edgedb": "^1.5.12"
  }
}
